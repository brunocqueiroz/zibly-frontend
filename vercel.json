{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.zibly.ai"
        }
      ],
      "destination": "https://zibly.ai/:path*",
      "permanent": true
    }
  ]
} 